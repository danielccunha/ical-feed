{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@config": "./src/config",
          "@controllers": "./src/controllers",
          "@models": "./src/models",
          "@services": "./src/services"
        }
      }
    ]
  ],
  "ignore": ["**/*.spec.ts"]
}
